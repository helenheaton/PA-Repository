SKIPCHECK;
FEEDSTRINGS;

['% Change on Prior Year']=C:  (['Year+1']-['Current Year'])\['Current Year'];

# Forecast is Actual until current month, then it takes the version specified in the model parameter cube
['Forecast','Current Year']=N:
  IF(AttrN('t_Period', !t_Period, 'Index')<=AttrN('t_Period', DB('Admin - Model Parameters', 'Actual Period', 'Value'), 'Index'),
    ['Actual'],
    DB('Cashflow', DB('Admin - Model Parameters', 'Version for Forecast', 'Value'), !t_Period, !t_Year, !Entity, !Department, !Cashflow Consol Layer, !Nominal Code )
    );

# Non PL items 

['Transfers (to)/from Reserve Accounts', 'Source']=N:
IF(!Department@=Attrs('Entity',!Entity,'Investment Activities Dept' ),
DB('Cashflow - Non PL Cash Items', !Version, !t_Period, !t_Year, !Entity, 'Net Life Cycle Transfer')
+ DB('Cashflow - Non PL Cash Items', !Version, !t_Period, !t_Year, !Entity, 'Net DSR Transfer'),
Continue);

['Dividends', 'Source']=N:
IF(!Department@=Attrs('Entity',!Entity,'Investment Activities Dept' ),
DB('Cashflow - Non PL Cash Items', !Version, !t_Period, !t_Year, !Entity, 'Net Dividends'),
Continue);

# Entered as a negative in Non PL Cash Items
['Capital Expenditure', 'Source']=N:
IF(!Department@=Attrs('Entity',!Entity,'Investment Activities Dept' ),
DB('Cashflow - Non PL Cash Items', !Version, !t_Period, !t_Year, !Entity, 'Capital Expenditure'),
Continue);

# Entered as a negative in Non PL Cash Items
['R119', 'GP01', 'Source']=N:
-DB('Cashflow - Non PL Cash Items', !Version, !t_Period, !t_Year, !Entity, 'Head Office Bonus Payments');


#Region Capital codes are from the Loans cube

# Income from Capital 

['Source','Sub Debt Capital Receivable']=N:
DB('Loans by Entity', !Version, !t_Period, !t_Year, 'Entity':'All Entities', !Department, !Entity, 'All Sources Exc WC', 'Nominal Code':'Sub Debt Interest External', 'Capital Repayments' )
+DB('Loans by Entity', !Version, !t_Period, !t_Year, 'Entity':'All Entities', !Department, !Entity,  'All Sources Exc WC', 'Nominal Code':'Sub Debt Interest Internal', 'Capital Repayments' )
+DB('Loans by Entity', !Version, !t_Period, !t_Year, 'Entity':'All Entities', !Department, !Entity, 'All Sources Exc WC', 'Nominal Code':'Other Interest Payable', 'Capital Repayments' );


# Internal Loans

['Source','Sub Debt Capital External']=N:
DB('Loans by Entity', !Version, !t_Period, !t_Year, !Entity, !Department, 'All Loaning Entities', 'All Sources Exc WC', 'Nominal Code':'Sub Debt Interest External', 'Capital Repayments' );

['Source','Sub Debt Capital Internal']=N:
DB('Loans by Entity', !Version, !t_Period, !t_Year, !Entity, !Department, 'All Loaning Entities', 'All Sources Exc WC', 'Nominal Code':'Sub Debt Interest Internal', 'Capital Repayments' );

['Source','Other Loan Capital Payable']=N:
DB('Loans by Entity', !Version, !t_Period, !t_Year, !Entity, !Department, 'All Loaning Entities', 'All Sources Exc WC', 'Nominal Code':'Other Interest Payable', 'Capital Repayments' )
+
IF(!t_Period@='March' & !Entity@='Group' & !Department@='GP02',
DB('Loans - Deferred Consideration', !Version, 'FY', Attrs('t_Year',!t_Year,'Financial Year'), 'All Deferred Consideration People', 'Capital' )
,0)
;


# External Loans

['Source','Senior Debt Capital']=N:
DB('Loans by Entity', !Version, !t_Period, !t_Year, !Entity, !Department, 'All Loaning Entities', 'All Sources Exc WC', 'Nominal Code':'Senior Debt Interest', 'Capital Repayments' );


# Working Capital

['Source','Intercompany Lending']=N:
DB('Loans by Entity', !Version, !t_Period, !t_Year, 'Entity':'All Entities', !Department, !Entity, 'Working Capital', 'Nominal Code':'Other Interest Payable', 'Capital Repayments' )
-DB('Loans by Entity', !Version, !t_Period, !t_Year, !Entity, !Department, 'All Loaning Entities', 'Working Capital', 'Nominal Code':'Other Interest Payable', 'Capital Repayments' );


#EndRegion

# Loans come from the Loan cubes
['Source']=N:
IF( DB('Loans by Entity', !Version, 'FY', !t_Year, !Entity, !Department, 'All Loaning Entities', 'All Sources', !'Nominal Code', 'Interest' )<>0 % 
DB('Loans by Entity', !Version, 'FY', !t_Year, 'All Entities', !Department, !Entity, 'All Sources', !'Nominal Code', 'Internal Cash Income' )<>0,
  DB('Loans by Entity', !Version, !t_Period, !t_Year, !Entity, !Department, 'All Loaning Entities', 'All Sources', !'Nominal Code', 'Cash Interest Repayments' )
+
  DB('Loans by Entity', !Version, !t_Period, !t_Year, 'All Entities', !Department, !Entity, 'All Sources', !'Nominal Code', 'Internal Cash Income' ),
  Continue)
+
IF(!t_Period@='September' & !Entity@='Group' & !Department@='GP02' & !Nominal Code@='S216',
DB('Loans - Deferred Consideration', !Version, 'H1', Attrs('t_Year',!t_Year,'Financial Year'), 'All Deferred Consideration People', 'Interest' ),0)
+
IF(!t_Period@='March' & !Entity@='Group' & !Department@='GP02' & !Nominal Code@='S216',
DB('Loans - Deferred Consideration', !Version, 'H2', Attrs('t_Year',!t_Year,'Financial Year'), 'All Deferred Consideration People', 'Interest' ),0);
  





# Else it should come from PL by Entity
['Source']=N:
DB('PL by Entity', !Version, !t_Period, !t_Year, !Entity, !Department, 'All Contracts',!Nominal Code);


# Generally follows P&L phasing but needs adjustment for CQC fees. Each entity has a different month in which these are paid so needs to be factored in in some way.
# P&L cost source is S236 which needs to be removed and the cash payment made in the correct month for each entity.

['S236','Phasing']=N:
IF(Attrs('Entity',!Entity,'CQC Payment Month')@='',
stet,
continue);

['S236','Phasing']=N:
IF(!t_Period@<>Attrs('Entity',!Entity,'CQC Payment Month'),
-['Source'],
continue);

['S236','Phasing']=N:
IF(!t_Period@=Attrs('Entity',!Entity,'CQC Payment Month'),
(['Source','FY']-['Source']),
continue);



#Region Phasing

['Phasing']=N:
IF(DB('Cashflow - Payment Profile', !Version, 'April', !Department, !'Nominal Code', 'Payment' )@=''
& DB('Cashflow - Payment Profile', !Version, 'May', !Department, !'Nominal Code', 'Payment' )@=''
& DB('Cashflow - Payment Profile', !Version, 'June', !Department, !'Nominal Code', 'Payment' )@=''
& DB('Cashflow - Payment Profile', !Version, 'July', !Department, !'Nominal Code', 'Payment' )@=''
& DB('Cashflow - Payment Profile', !Version, 'August', !Department, !'Nominal Code', 'Payment' )@=''
& DB('Cashflow - Payment Profile', !Version, 'September', !Department, !'Nominal Code', 'Payment' )@=''
& DB('Cashflow - Payment Profile', !Version, 'October', !Department, !'Nominal Code', 'Payment' )@=''
& DB('Cashflow - Payment Profile', !Version, 'November', !Department, !'Nominal Code', 'Payment' )@=''
& DB('Cashflow - Payment Profile', !Version, 'December', !Department, !'Nominal Code', 'Payment' )@=''
& DB('Cashflow - Payment Profile', !Version, 'January', !Department, !'Nominal Code', 'Payment' )@=''
& DB('Cashflow - Payment Profile', !Version, 'February', !Department, !'Nominal Code', 'Payment' )@=''
& DB('Cashflow - Payment Profile', !Version, 'March', !Department, !'Nominal Code', 'Payment' )@='',
  0,
  CONTINUE);

['Phasing']=N:
IF(DB('Cashflow - Payment Profile', !Version, !t_Period, !Department, !'Nominal Code', 'Payment' )@='',
-['Source'],
Continue);




# Nasty phasing calc 

# If there is a payment in the previous month, there will be nothing accrued to pay. 
['Phasing']=N:
IF(DB('Cashflow - Payment Profile', !Version,   Attrs('t_Period', !t_Period, 'Previous Period'), !Department, !'Nominal Code', 'Payment' )@='Payment',
    0, 
    Continue);

# If the period is April, it is the first month so there will be nothing accrued to pay. 
 ['Phasing']=N:
 IF(DB('Cashflow - Payment Profile', !Version,  !t_Period,  !Department, !'Nominal Code', 'Payment' )@='Payment',
 IF(!t_Period@='April', 
   0,
   Continue),continue);

# If there was a payment 2 months ago, there will be one month to pay
# If the last month is April, then there will be one month to pay. 
['Phasing']=N:
# Previous Month 1
 IF(DB('Cashflow - Payment Profile', !Version,  !t_Period,  !Department, !'Nominal Code', 'Payment' )@='Payment',
IF(DB('Cashflow - Payment Profile', !Version,   Attrs('t_Period', !t_Period, 'Period-2'), !Department, !'Nominal Code', 'Payment' )@='Payment'
% Attrs('t_Period', !t_Period, 'Previous Period')@='April', 
  DB('Cashflow', !Version,  Attrs('t_Period', !t_Period, 'Previous Period'), !t_Year, !Entity, !Department, 'Source', !'Nominal Code' )
  ,Continue),Continue);

['Phasing']=N:
# Previous Month 2
 IF(DB('Cashflow - Payment Profile', !Version,  !t_Period,  !Department, !'Nominal Code', 'Payment' )@='Payment',
IF(DB('Cashflow - Payment Profile', !Version,   Attrs('t_Period', !t_Period, 'Period-3'), !Department, !'Nominal Code', 'Payment' )@='Payment'
  % Attrs('t_Period', !t_Period, 'Period-2')@='April', 
  DB('Cashflow', !Version,  Attrs('t_Period', !t_Period, 'Previous Period'), !t_Year, !Entity, !Department, 'Source', !'Nominal Code' )
  +  DB('Cashflow', !Version,  Attrs('t_Period', !t_Period, 'Period-2'), !t_Year, !Entity, !Department, 'Source', !'Nominal Code' )
,Continue),continue); 


['Phasing']=N:
# Previous Month 3
 IF(DB('Cashflow - Payment Profile', !Version,  !t_Period,  !Department, !'Nominal Code', 'Payment' )@='Payment',
