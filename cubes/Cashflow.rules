#EndRegion

#Region Phasing Payment In Advance
# If there is a payment in the next month, there will be nothing accrued to pay. 
['Phasing']=N:
IF(DB('Cashflow - Payment Profile', !Version,   Attrs('t_Period', !t_Period, 'Next Period'), !Department, !'Nominal Code', 'Payment' )@='Payment In Advance',
    0, 
    Continue);

# If the period is March, it is the last month so there will be nothing accrued to pay. 
 ['Phasing']=N:
IF(DB('Cashflow - Payment Profile', !Version,   !t_Period,  !Department, !'Nominal Code', 'Payment' )@='Payment In Advance',
 IF(!t_Period@='March', 
   0,
   Continue),continue);

# If there is a payment in 2 months, there will be one month to pay
# If the last month is March, then there will be one month to pay. 
['Phasing']=N:
# Next Month 1
IF(DB('Cashflow - Payment Profile', !Version,   !t_Period,  !Department, !'Nominal Code', 'Payment' )@='Payment In Advance',
IF(DB('Cashflow - Payment Profile', !Version,   Attrs('t_Period', !t_Period, 'Period-10'), !Department, !'Nominal Code', 'Payment' )@='Payment In Advance'
% Attrs('t_Period', !t_Period, 'Next Period')@='March', 
  DB('Cashflow', !Version,  Attrs('t_Period', !t_Period, 'Next Period'), !t_Year, !Entity, !Department, 'Source', !'Nominal Code' )
  ,Continue),CONTINUE);

['Phasing']=N:
