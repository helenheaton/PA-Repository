# Previous Month 9 
IF(DB('Cashflow - Payment Profile', !Version,  !t_Period,  !Department, !'Nominal Code', 'Payment' )@='Payment',
IF(DB('Cashflow - Payment Profile', !Version,   Attrs('t_Period', !t_Period, 'Period-10'), !Department, !'Nominal Code', 'Payment' )@='Payment'
