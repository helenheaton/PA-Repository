    +  DB('Cashflow', !Version,  Attrs('t_Period', !t_Period, 'Period-7'), !t_Year, !Entity, !Department, 'Source', !'Nominal Code' )
    +  DB('Cashflow', !Version,  Attrs('t_Period', !t_Period, 'Period-8'), !t_Year, !Entity, !Department, 'Source', !'Nominal Code' )
,Continue),continue); 

