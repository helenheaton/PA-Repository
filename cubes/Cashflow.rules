    +  DB('Cashflow', !Version,  Attrs('t_Period', !t_Period, 'Period-4'), !t_Year, !Entity, !Department, 'Source', !'Nominal Code' )
    +  DB('Cashflow', !Version,  Attrs('t_Period', !t_Period, 'Period-5'), !t_Year, !Entity, !Department, 'Source', !'Nominal Code' )
